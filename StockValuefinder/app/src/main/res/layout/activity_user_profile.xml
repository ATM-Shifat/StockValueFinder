                app:layout_constraintHorizontal_bias="0.608"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/nameprofile"
                app:layout_constraintVertical_bias="0.293" />

            <View
